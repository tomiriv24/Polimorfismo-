class Animal:
    def hacer_sonido(self):
        print("Sonido gen√©rico de animal")

class Perro(Animal):
    def hacer_sonido(self):
        print("Guau")

class Gato(Animal):
    def hacer_sonido(self):
        print("Miau")

animales = [Perro(), Gato()]

for animal in animales:
    animal.hacer_sonido()
