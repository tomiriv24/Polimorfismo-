class Vehiculo:
    def mover(self):
        print("El vehículo se está moviendo")

class Coche(Vehiculo):
    def mover(self):
        print("El coche está conduciendo por la carretera")

class Bicicleta(Vehiculo):
    def mover(self):
        print("La bicicleta está pedaleando por el carril bici")

def probar_vehiculos(vehiculos):
    for vehiculo in vehiculos:
        vehiculo.mover()


vehiculos = [Coche(), Bicicleta()]
probar_vehiculos(vehiculos)
